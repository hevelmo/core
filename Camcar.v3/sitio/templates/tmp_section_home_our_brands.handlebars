<div id="ScrollDown"></div>
<section class="large-pad about-content">
	<div class="container">
		<div class="row">
			<div class="col-md-12 col-sm-12 text-center" style="padding-bottom: 40px;">
				<h1 class="h-landmark animated hiding" data-animation="fadeInDown" data-delay="500"><span>Nuestras Marcas</span></h1>
			</div>
		</div>
		<div class="row">
			<div class="col-md-12 col-sm-12">
			{{#each campa}}
				<div class="x-column-home" id="{{agn_id}}">
				{{#if action-nuevo}}
					<a class="x-img x-img-link x-img-none none x-img-picture cur-hover animated hiding act-agn-{{action-nuevo}}"
					data-agn-name-agencie="{{agp_short}}"
					data-agp_nombre="{{agp_short}}"
					data-animation="fadeInUp" data-delay="400" id="{{agnPrincipal}}"
					data-index="{{agn_index}}">
						<img src="../img/sitio/our-brands/{{logo}}">
					</a>
				{{/if}}
				{{#if action-seminuevo}}
					<a class="x-img x-img-link x-img-none none x-img-picture cur-hover animated hiding act-agn-{{action-seminuevo}}"
					data-agn-preowned-maps="{{agn_id}}"
					data-agn-preowned-id="{{agn_id}}"
					data-agn-preowned-url="{{agn_ruta}}"
					data-agp_id="{{agnPrincipal}}"
					data-animation="fadeInUp" data-delay="400" id=""
					data-index="{{agn_index}}">
						<img src="../img/sitio/our-brands/{{logo}}">
					</a>
				{{/if}}
				{{#if action-taller}}
					<a class="x-img x-img-link x-img-none none x-img-picture cur-hover animated hiding act-agn-{{action-taller}}"
					data-agp_id="{{agnPrincipal}}"
					data-animation="fadeInUp" data-delay="400" id=""
					data-index="{{agn_index}}">
						<img src="../img/sitio/our-brands/{{logo}}">
					</a>
				{{/if}}
				{{#if action-rentas}}
					<a class="x-img x-img-link x-img-none none x-img-picture cur-hover animated hiding act-agn-{{action-rentas}}"
					data-agencie-rental-name="{{agnBreadcrumb_name}}" data-agencie-rental-key="{{agnBreadcrumb_key}}"
					data-agp_id="{{agnPrincipal}}"
					data-animation="fadeInUp" data-delay="400" id=""
					data-index="{{agn_index}}">
						<img src="../img/sitio/our-brands/{{logo}}">
					</a>
				{{/if}}
				</div>
			{{/each}}
			</div>
			<div class="col-md-12 col-sm-12">
				<div class="row">
					<a class="ScrollDown" href="#ScrollDown" id="ScrollDown" style="bottom: -25px; left: 47%;">
			            <i class="fa fa-angle-double-down fa-5x animation-pulse" style="margin: 7px;"></i>
			        </a>
				</div>
			</div>
		</div>
	</div>
</section>
